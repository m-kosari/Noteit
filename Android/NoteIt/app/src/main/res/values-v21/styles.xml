<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <!-- progress style-->
    <style name="Progress" parent="Theme.AppCompat.Light">
        <item name="colorAccent">#bababa</item>
        <item name="android:minHeight">5dp</item>
        <item name="android:maxHeight">5dp</item>
        <item name="android:progressBackgroundTint">#0e0e0e</item>
    </style>


</resources>